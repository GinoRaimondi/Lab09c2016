<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="dam.isi.frsf.utn.edu.ar.lab09c2016.MainActivity"
    tools:showIn="@layout/activity_main">

    <TextView
        android:text="Magnitud: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/horaMaxX"
        android:layout_alignParentStart="true"
        android:id="@+id/magnitudX" />

    <TextView
        android:text="Hora Máximo Y: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/magnitudX"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp"
        android:id="@+id/horaMaxY" />

    <TextView
        android:text="Magnitud: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/horaMaxY"
        android:layout_alignParentStart="true"
        android:id="@+id/magnitudY" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hora Máximo X: "
        android:id="@+id/horaMaxX"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Hora Máximo Z: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/magnitudY"
        android:layout_alignParentStart="true"
        android:layout_marginTop="26dp"
        android:id="@+id/horaMaxZ" />

    <TextView
        android:text="Magnitud: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/magnitudZ"
        android:layout_below="@+id/horaMaxZ"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="dsadsadadwa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:id="@+id/textViewtodos"
        android:layout_below="@+id/magnitudZ"
        android:layout_alignParentStart="true" />
</RelativeLayout>
